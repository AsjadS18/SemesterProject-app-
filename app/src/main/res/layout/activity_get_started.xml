<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GetStarted">


    <TextView
        android:id="@+id/textView"
        android:layout_width="142dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="128dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="141dp"
        android:text="Get Started"
        android:textStyle="bold"
        android:textSize="25dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="7dp" />

    <EditText
            android:id="@+id/editTextTextMultiLine2"
            android:layout_width="368dp"
            android:layout_height="81dp"
            android:layout_below="@id/textView"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="23dp"
            android:layout_marginTop="38dp"
            android:layout_marginEnd="20dp"
            android:ems="10"
            android:text="Please fill out consent and Medical History form. You can only fill out these forms once when you are register. Then, you can just get appointments whenever you need."
            android:gravity="start|top"
            android:inputType="textMultiLine" />

    <Button
            android:id="@+id/buttonfinal"
            android:layout_width="116dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="146dp"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="151dp"
            android:text="Home"
        android:onClick="home1"/>

    <ImageView
        android:id="@+id/image1"
        android:layout_width="256dp"
        android:layout_height="80dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="83dp"
        android:layout_marginBottom="434dp"
        android:contentDescription="@string/telehealth_icon_desc"
        android:src="@drawable/allbackgrounds" />

    <ImageView
        android:id="@+id/image2"
        android:layout_width="256dp"
        android:layout_height="80dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="83dp"
        android:layout_marginBottom="308dp"
        android:contentDescription="@string/telehealth_icon_desc"
        android:src="@drawable/allbackgrounds" />

    <TextView
        android:id="@+id/telehealth"
        android:layout_width="215dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="90dp"
        android:layout_marginBottom="332dp"
        android:text="     Telehealth consent"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        android:onClick="telehealth1"/>

    <TextView
        android:id="@+id/medical"
        android:layout_width="215dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="90dp"
        android:layout_marginBottom="465dp"
        android:text="       Medical Form"
        android:textColor="@color/cardview_dark_background"
        android:textSize="22dp"
        android:textStyle="bold"
        android:onClick="medical1"/>

</RelativeLayout>
